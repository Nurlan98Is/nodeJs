{
    "version": 2,
    "builds": [
        {
            "src": "api/**/*.js",
            "use": "@vercel/node"
        }
    ],
    "routes": [
        {
            "src": "/api/user",
            "dest": "/api/user.js"
        },
        {
            "src": "/api/product",
            "dest": "/api/product.js"
        },
        {
            "src": "/api/hello",
            "dest": "/api/hello.js"
        },
        {
            "src": "/api",
            "dest": "/api/index.js"
        },
        {
            "src": "/(.*)",
            "dest": "/api/index.js"
        }
    ]
}